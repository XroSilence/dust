{
  "name": "dust",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "homepage": "https://XroSilence.github.io/dust",
  "os": [
    "linux",
    "win32"
  ],
  "scripts": {
    "start": "cd backend && npm run start",
    "clean": "rimraf dist && mkdir dist && cd backend && npm run clean",
    "ci": "npm ci && cd backend && npm run ci",
    "dev": "concurrently \"vite\" \"npm run dev:backend\"",
    "dev:backend": "cd backend && npm run dev",
    "build": "vite build && npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "he": "^1.2.0",
    "http-proxy-middleware": "^3.0.3",
    "lucide-react": "^0.471.0",
    "otpauth": "^9.3.6",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "express": "^4.21.2",
    "gh-pages": "^6.3.0",
    "jspdf": "^2.5.2",
    "postcss": "^8.5.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
